{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "noted-task-manager",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/dashboard/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
